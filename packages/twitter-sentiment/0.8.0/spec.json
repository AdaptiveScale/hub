{
  "specVersion": "1.0",
  "label": "Twitter Sentiment Analysis",
  "description": "The TwitterSentiment application analyzes the sentiments of Twitter tweets and categorizes them as either positive, negative or neutral. Key features include real-time collection of data from Twitter and real-time statistics on analyzed tweets, with breakdown by sentiment. See https://github.com/caskdata/cdap-apps/tree/develop/TwitterSentiment for more information and for the source code.", 
  "author": "Cask",
  "org": "Cask Data, Inc.",
  "created": 1474312686,
  "categories": [ "example" ],
  "cdapVersion": "[4.0.0-SNAPSHOT,4.1.0)",
  "actions": [
    {
      "type": "create_stream",
      "arguments": [
        {
          "name": "name",
          "value": "TweetStream"
        }
      ]
    },
    {
      "type": "load_datapack",
      "arguments": [
        {
          "name": "name",
          "value": "TweetStream"
        },
        {
          "name": "files",
          "value": [ "sample_tweets.txt" ]
        }
      ]
    },
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "TwitterSentiment"
        },
        {
          "name": "version",
          "value": "0.8.0"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "jar",
          "value": "TwitterSentiment-0.8.0.jar"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "user",
            "name": "TwitterSentiment",
            "version": "0.8.0"
          }
        },
        {
          "name": "name",
          "value": "TwitterSentiment"
        }
      ]
    }
  ]
}
