{
  "specVersion": "1.0",
  "label": "Clicks and Views MapReduce Join",
  "description": "Example of a MapReduce that joins click and view data from two streams.",
  "author": "Cask Data Inc.",
  "org": "Cask Data Inc.",
  "created": 1474060461,
  "categories": [ "example" ],
  "cdapVersion": "[4.0.0-SNAPSHOT,4.1.0)",
  "actions": [
    {
      "type": "create_stream",
      "arguments": [
        {
          "name": "name",
          "value": "clicks"
        }
      ]
    },
    {
      "type": "load_datapack",
      "arguments": [
        {
          "name": "name",
          "value": "clicks"
        },
        {
          "name": "files",
          "value": [ "clicks.tsv" ]
        }
      ]
    },
    {
      "type": "create_stream",
      "arguments": [
        {
          "name": "name",
          "value": "views"
        }
      ]
    },
    {
      "type": "load_datapack",
      "arguments": [
        {
          "name": "name",
          "value": "views"
        },
        {
          "name": "files",
          "value": [ "views.tsv" ]
        }
      ]
    },
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "ClicksAndViews"
        },
        {
          "name": "version",
          "value": "4.0.0"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "jar",
          "value": "ClicksAndViews-4.0.0.jar"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "user",
            "name": "ClicksAndViews",
            "version": "4.0.0"
          }
        },
        {
          "name": "name",
          "value": "ClicksAndViews"
        }
      ]
    }
  ]
}
