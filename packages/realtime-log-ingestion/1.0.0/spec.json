        {
          "name": "files",
          "value": [ "logdata.txt" ]
        }
      ]
    },
    {
      "type": "create_pipeline",
      "label": "Log Ingestion and Indexing Pipeline",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "system",
            "name": "cdap-data-streams",
            "version": "4.0.0-SNAPSHOT"
          }
        },
        {
          "name": "name",
          "value": "logIngestion",
          "canModify": true
        },
        {
          "name": "config",
          "value": "pipeline.json"
        } 
      ]
    }
  ]
