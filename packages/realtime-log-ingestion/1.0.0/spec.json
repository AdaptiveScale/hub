{
  "specVersion": "1.0",
  "label": "Real-time Log Analytics",
  "description": "Real-time log analytics and log indexing is the most common use case for processing machine-generated logs. Log analytics provides insights into how users are using your application or how devices are performing in the field. Real-time analytics provides insights as soon as they happen providing faster response times.",
  "author": "Cask",
  "org": "Cask Data, Inc.",
  "created": 1473901763,
  "categories": [ "usecase" ],
