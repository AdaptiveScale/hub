{
  "specVersion": "1.0",
  "label": "MySQL to Hive",
  "description": "Reads from a MySQL Database (using version 5.1.39 of the MySQL JDBC driver) and writes to Hive",
  "author": "Cask Data Inc.",
  "org": "Cask Data Inc.",
  "categories": [ "hydrator-pipeline" ],
  "cdapVersion": "[4.0.0-SNAPSHOT,4.1.0)",
  "actions": [
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "mysql-connector-java"
        },
        {
          "name": "version",
          "value": "5.1.39"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "externalArchive",
          "value": "https://dev.mysql.com/downloads/file/?id=462849"
        },
        {
          "name": "externalArchiveJar",
          "value": "mysql-connector-java-5.1.39-bin.jar"
        },
        {
          "name": "config",
          "value": "mysql-connector-java-5.1.39.json"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "system",
            "name": "cdap-data-pipeline",
            "version": "4.0.0-SNAPSHOT"
          }
        },
        {
          "name": "name",
          "value": "MySQL-to-Hive",
          "canModify": true
        },
        {
          "name": "config",
          "value": "pipeline.json"
        } 
      ]
    }
  ]
}
