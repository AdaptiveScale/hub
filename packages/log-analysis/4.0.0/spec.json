{
  "specVersion": "1.0",
  "label": "Log Analysis with Spark and MapReduce",
  "description": "A Workflow of Spark and MapReduce programs that reads Apache logs from a Stream, writes counts to FileSets, and serves those counts with a Service.",
  "author": "Cask Data Inc.",
  "org": "Cask Data Inc.",
  "created": 1474060461,
  "categories": [ "example" ],
  "cdapVersion": "[4.0.0-SNAPSHOT,4.1.0)",
  "actions": [
    {
      "type": "create_stream",
      "arguments": [
        {
          "name": "name",
          "value": "logStream"
        },
        {
          "name": "loadfiles",
          "value": [ "apachelog.txt" ]
        }
      ]
    },
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "LogAnalysis"
        },
        {
          "name": "version",
          "value": "4.0.0"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "jar",
          "value": "LogAnalysis-4.0.0.jar"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "user",
            "name": "LogAnalysis",
            "version": "4.0.0"
          }
        },
        {
          "name": "name",
          "value": "LogAnalysis"
        }
      ]
    }
  ]
}
