{
  "specVersion": "1.0",
  "label": "Purchase History Workflow and Service",
  "description": "A workflow that stores what items each user has purchased, and serves the data with a Service.",
  "author": "Cask Data Inc.",
  "org": "Cask Data Inc.",
  "created": 1474060461,
  "categories": [ "example" ],
  "cdapVersion": "[4.0.0-SNAPSHOT,4.1.0)",
  "actions": [
    {
      "type": "create_stream",
      "arguments": [
        {
          "name": "name",
          "value": "purchaseStream"
        }
      ]
    },
    {
      "type": "load_datapack",
      "arguments": [
        {
          "name": "name",
          "value": "purchaseStream"
        },
        {
          "name": "files",
          "value": [ "purchases.txt" ]
        }
      ]
    },
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "Purchase"
        },
        {
          "name": "version",
          "value": "4.0.0"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "jar",
          "value": "Purchase-4.0.0.jar"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "user",
            "name": "Purchase",
            "version": "4.0.0"
          }
        },
        {
          "name": "name",
          "value": "PurchaseHistory"
        }
      ]
    }
  ]
}
