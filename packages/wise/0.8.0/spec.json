      "arguments": [
        {
          "name": "name",
          "value": "logEventStream"
        }
      ]
    },
    {
      "type": "load_datapack",
      "arguments": [
        {
          "name": "name",
          "value": "logEventStream"
        },
        {
          "name": "files",
          "value": [ "log_events.txt" ]
        }
      ]
    },
    {
      "type": "create_artifact",
      "arguments": [
        {
          "name": "name",
          "value": "cdap-wise"
        },
        {
          "name": "version",
          "value": "0.8.0"
        },
        {
          "name": "scope",
          "value": "user"
        },
        {
          "name": "jar",
          "value": "cdap-wise-0.8.0.jar"
        }
      ]
    },
    {
      "type": "create_app",
      "arguments": [
        {
          "name": "artifact",
          "value": {
            "scope": "user",
            "name": "cdap-wise",
            "version": "0.8.0"
          }
        },
        {
          "name": "name",
          "value": "Wise"
        }
      ]
    }
  ]
}
