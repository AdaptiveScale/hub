{
  "specVersion": "1.0",
  "description": "This example demonstrates Spark and MapReduce performing streaming log analysis, computing the page rank based on information about backlink URLs. For information on running the application, see http://docs.cask.co/cdap/4.1.0/en/examples-manual/examples/spark-page-rank.html",
  "label": "Spark PageRank Example",
  "author": "Cask",
  "org": "Cask Data, Inc.",
  "cdapVersion": "[4.1.0-SNAPSHOT,4.2.0)",
  "created": 1487632890,
  "categories": [
    "example"
  ],
  "actions": [
    {
      "type": "create_stream",
      "label": "Create URL Stream",
      "arguments": [
        {
          "name": "name",
          "value": "backlinkURLStream",
          "canModify": false
        },
        {
          "name": "description",
          "value": "Stream of URL pairs representing links between pages",
          "canModify": false
        }
      ]
    },
    {
      "type": "load_datapack",
      "label": "Load Sample Links",
      "arguments": [
        {
          "name": "name",
          "value": "backlinkURLStream",
          "canModify": false
        },
        {
          "name": "files",
          "value": [
            "urlpairs.txt"
          ],
          "canModify": false
        }
      ]
    },
    {
      "type": "informational",
      "label": "Download Application JAR",
      "arguments": [
        {
          "name": "steps",
          "value": [
            "Download the application JAR from http://market.cask.co/v2/packages/example-spark-pagerank/4.1.0/SparkPageRank-4.1.0.jar",
            "Upload the JAR in the next step"
          ],
          "canModify": false
        }
      ]
    },
    {
      "type": "deploy_app",
      "label": "Deploy App",
      "arguments": [
        {
          "name": "jar",
          "value": "SparkPageRank-4.1.0.jar",
          "canModify": false
        }
      ]
    }
  ]
}
